# Reload API Configuration
# Get these values from your Reload Developer Dashboard

# Your Reload application client ID (client-side accessible)
NEXT_PUBLIC_RELOAD_APP_ID=your_client_id_here

# Your Reload application client ID (server-side, optional - will fallback to NEXT_PUBLIC_ version)
RELOAD_APP_ID=your_client_id_here

# Your Reload application client secret (server-side only)
RELOAD_CLIENT_SECRET=your_client_secret_here

# Your OAuth callback URL (client-side accessible, must match what's registered in Reload Developer Dashboard)
NEXT_PUBLIC_REDIRECT_URI=http://localhost:3001/callback

# Your OAuth callback URL (server-side, optional - will fallback to NEXT_PUBLIC_ version)
REDIRECT_URI=http://localhost:3001/callback

# Tunneling service configuration (optional)
# Set to 'true' when using tunneling services like ngrok, localtunnel, etc.
NEXT_PUBLIC_USE_TUNNEL=false

# Optional: Set to 'development' for debug logging
NODE_ENV=development 